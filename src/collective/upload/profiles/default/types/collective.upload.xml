<?xml version="1.0"?>
<object name="collective.upload" meta_type="Dexterity FTI"
        i18n:domain="collective.upload"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title"
    i18n:translate="">Upload multiple files or images</property>
 <property name="description" i18n:translate=""></property>
 <property name="icon_expr">string:++resource++collective.upload/document_small_upload.png</property>
 <property name="factory">collective.upload</property>
 <property
    name="add_view_expr">string:${folder_url}/@@media_uploader</property>
 <property name="global_allow">True</property>
 <property name="add_permission">collective.upload.UploadFiles</property>
 <property name="klass">plone.dexterity.content.Item</property>
</object>